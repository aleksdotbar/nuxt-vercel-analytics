{
  "name": "nuxt-vercel-analytics",
  "version": "0.2.2",
  "description": "Nuxt module to integrate Vercel Analytics",
  "homepage": "https://github.com/xanderbarkhatov/nuxt-vercel-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xanderbarkhatov/nuxt-vercel-analytics.git"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "vercel",
    "analytics"
  ],
  "author": {
    "name": "Alexander B.",
    "email": "mail@xanderbarkhatov.com",
    "url": "https://xanderbarkhatov.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build",
    "prepack": "pnpm build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "3.5.2",
    "@vercel/analytics": "^1.0.0",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/module-builder": "0.4.0",
    "@nuxt/schema": "3.5.2",
    "@nuxt/test-utils": "3.5.2",
    "changelogen": "0.5.3",
    "eslint": "8.41.0",
    "nuxt": "3.5.2",
    "vitest": "0.31.4"
  }
}